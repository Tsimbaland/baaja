<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Slide.
    -->
    <changeSet id="20200221131458-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="body_id"
                                 baseTableName="slide"
                                 constraintName="fk_slide_body_id"
                                 referencedColumnNames="id"
                                 referencedTableName="body"/>

        <addForeignKeyConstraint baseColumnNames="presentation_id"
                                 baseTableName="slide"
                                 constraintName="fk_slide_presentation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="presentation"/>

    </changeSet>
</databaseChangeLog>
